    "columns": {
      "type": "columns",
      "blocks": {
        "column": {
          "type": "column",
          "settings": {
            "button_label": "Learn more",
            "button_style": "secondary",
            "button_url": "booqable://pages/trails",
            "icon": "phone",
            "image": "booqable://assets/image-endure-column-1.jpg",
            "text": "<p>Thrilling downhill descents, rock gardens, and stunning vistas define this adrenaline-pumping mountain biking destination.</p>",
            "title": "Smith Creek"
          },
          "disabled": null
        },
        "column_280121": {
          "type": "column",
          "settings": {
            "button_label": "Learn more",
            "button_style": "secondary",
            "button_url": "booqable://pages/trails",
            "icon": "phone",
            "image": "booqable://assets/image-endure-column-2.jpg",
            "text": "<p>Challenge your skills on seven unique summits, offering epic views and heart-pounding descents for all riders.</p>",
            "title": "Seven Summit Ridge"
          },
          "disabled": null
        },
        "column_634020": {
          "type": "column",
          "settings": {
